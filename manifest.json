{
	"manifest_version": 3,
	"name": "Link Saver",
	"version": "1.0",
	"description": "Save links and share them easily.",
	"action": {
		"default_popup": "popup.html",
		"default_icon": "icons.png"
	},
	"permissions": ["storage", "activeTab", "tabs"],
	"background": {
		"service_worker": "background.js"
	}
}
